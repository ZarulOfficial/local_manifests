<?xml version="1.0" encoding="UTF-8"?>
<manifest>

    <remote name="github" fetch="https://github.com/" />
    <remote name="2by2" fetch="https://github.com/2by2-Project/" />
    <remote name="lineage" fetch="https://github.com/LineageOS/" />
    <remote name="crdroid" fetch="https://github.com/crdroidandroid/" />
    <remote name="pixel-os" fetch="https://github.com/PixelOS-AOSP/" />

    <!-- Remove -->
    <remove-project name="frameworks_base" />
    <remove-project name="frameworks_av" />
    <remove-project name="packages_apps_Settings" />
    <remove-project name="packages_modules_Bluetooth" />
    <remove-project name="packages_modules_adb" />
